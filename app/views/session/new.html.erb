<div class="session">
  <h2>Existing Member Sign In</h2>

  <%= flash[:error] %>
  <br>
  <%= form_tag login_path do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag "Log In" %>
  <% end %>

  <img id="loginimg" src="/assets/Single-Married-Hungry-ENAMEL.jpg" alt="" width="400px">
</div>
